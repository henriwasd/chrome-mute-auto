{
  "manifest_version": 3,
  "name": "Chrome Mute Auto",
  "version": "1.0",
  "description": "Mute all tabs automatically, except for the fixed tabs.",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ]
}